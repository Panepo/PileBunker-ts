    "@types/react-test-renderer": "^16.8.1",
    "@types/redux-test-utils": "^0.2.0",
    "@types/webpack-env": "1.13.6",
    "cheerio": "^1.0.0-rc.3",
    "codecov": "^3.7.1",
    "csstype": "^2.4.2",
