    "@types/react-dom": "16.0.11",
    "@types/react-redux": "7.0.0",
    "@types/react-router": "4.4.3",
    "@types/redux-logger": "3.0.6",
    "connected-react-router": "6.2.2",
    "history": "^4.7.2",
    "localforage": "1.7.3",
    "lokijs": "^1.5.5",
    "prop-types": "^15.7.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
