    "react-number-format": "^4.0.6",
    "react-redux": "6.0.0",
    "react-router": "4.3.1",
    "react-scripts-ts": "3.1.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "^0.0.54"
