{
  "name": "pilebunker",
  "version": "3.0.0",
  "description": "shiropro weapon calculator pilebunker",
  "private": true,
  "author": "Panepo <panepo@github.io>",
  "homepage": "https://panepo.github.io/PileBunker-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Panepo/PileBunker-ts.git"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom --coverage",
    "eject": "react-scripts-ts eject",
    "crawler": "lsc ./src/resource/crawler.ls --nodejs --harmoy",
    "parser": "lsc ./src/resource/parser.ls --nodejs --harmoy",
    "prettier": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.{ts,tsx}\"",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --single-quote --no-semi --trailing-comma none --write",
      "git add"
    ]
  },
